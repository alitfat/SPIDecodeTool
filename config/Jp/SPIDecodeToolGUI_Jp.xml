<?xml version='1.0' encoding='utf-8'?>
<SPIDecodeTool>
  <ToolName>
    <!--ツール名称設定-->
    <widTool>
      <Name>widTool</Name>
      <QtWidget>QWidget</QtWidget>
      <WindowTitle>SPIDecode解析ツール</WindowTitle>
    </widTool>
  </ToolName>
  <!--【メニューバー】GUI-->
  <menubarList>
    <!--【設定】メニュー-->
    <menubar0>
      <Name>menubar0</Name>
      <QtWidget>QMenu|QAction</QtWidget>
      <Title>設定</Title>
      <Enabled>True</Enabled>
      <menu0>
        <Name>menu0</Name>
        <QtWidget>QAction</QtWidget>
        <Text>その他(未使用)</Text>
        <Enabled>True</Enabled>
      </menu0>
    </menubar0>
    <!--【ヘルプ】メニュー-->
    <menubar1>
      <Name>menubar1</Name>
      <QtWidget>QMenu|QAction</QtWidget>
      <Title>ヘルプ</Title>
      <Enabled>True</Enabled>
      <menu0>
        <Name>menu0</Name>
        <QtWidget>QAction</QtWidget>
        <Text>SPIDecodeToolについて</Text>
        <Enabled>True</Enabled>
      </menu0>
    </menubar1>
  </menubarList>
  <!--【各SPIツール設定】GUI-->
  <actionToolList>
    <lbObjFileName>
      <Name>lbObjFileName</Name>
      <QtWidget>QLabel</QtWidget>
      <Text>対象ファイル アドレス：</Text>
      <Hidden>False</Hidden>
    </lbObjFileName>
    <txObjFileName>
      <Name>txObjFileName</Name>
      <QtWidget>QTextEdit</QtWidget>
      <Text />
      <Enabled>True</Enabled>
      <Hidden>False</Hidden>
    </txObjFileName>
    <btObjFileName>
      <Name>btObjFileName</Name>
      <QtWidget>QPushButton</QtWidget>
      <Text>選択</Text>
      <Enabled>True</Enabled>
      <Hidden>False</Hidden>
    </btObjFileName>
    <lbDstFilePath>
      <Name>lbDstFilePath</Name>
      <QtWidget>QLabel</QtWidget>
      <Text>生成ファイルパス：</Text>
      <Hidden>False</Hidden>
    </lbDstFilePath>
    <txDstFilePath>
      <Name>txDstFilePath</Name>
      <QtWidget>QTextEdit</QtWidget>
      <Text />
      <Enabled>False</Enabled>
      <Hidden>False</Hidden>
    </txDstFilePath>
    <cbDstFilePath>
      <Name>cbDstFilePath</Name>
      <QtWidget>QCheckBox</QtWidget>
      <Text>手書き入力</Text>
      <Checked>False</Checked>
      <Enabled>True</Enabled>
      <Hidden>False</Hidden>
    </cbDstFilePath>
    <btDstFilePath>
      <Name>btDstFilePath</Name>
      <QtWidget>QPushButton</QtWidget>
      <Text>選択</Text>
      <Enabled>True</Enabled>
      <Hidden>False</Hidden>
    </btDstFilePath>
    <lbDstFileName>
      <Name>lbDstFileName</Name>
      <QtWidget>QLabel</QtWidget>
      <Text>生成ファイル 名称：</Text>
      <Hidden>False</Hidden>
    </lbDstFileName>
    <txDstFileName>
      <Name>txDstFileName</Name>
      <QtWidget>QTextEdit</QtWidget>
      <Text />
      <Enabled>False</Enabled>
      <Hidden>False</Hidden>
    </txDstFileName>
    <cbDstFileName>
      <Name>cbDstFileName</Name>
      <QtWidget>QCheckBox</QtWidget>
      <Text>手書き入力</Text>
      <Checked>False</Checked>
      <Enabled>True</Enabled>
      <Hidden>False</Hidden>
    </cbDstFileName>
    <lbDstFileNote>
      <Name>lbDstFileNote</Name>
      <QtWidget>QLabel</QtWidget>
      <Text>※　👆 拡張子を入れないでください</Text>
      <Hidden>False</Hidden>
    </lbDstFileNote>
    <cbDstFileOverWrite>
      <Name>cbDstFileOverWrite</Name>
      <QtWidget>QCheckBox</QtWidget>
      <Text>上書き保存</Text>
      <Checked>True</Checked>
      <Enabled>True</Enabled>
      <Hidden>False</Hidden>
    </cbDstFileOverWrite>
    <cb2ExcelFile>
      <Name>cb2ExcelFile</Name>
      <QtWidget>QCheckBox</QtWidget>
      <Text>Excelファイルへ出力</Text>
      <Checked>False</Checked>
      <Enabled>True</Enabled>
      <Hidden>False</Hidden>
    </cb2ExcelFile>
    <btDstFileProcess>
      <Name>btDstFileProcess</Name>
      <QtWidget>QPushButton</QtWidget>
      <Text>実施</Text>
      <Enabled>True</Enabled>
      <Hidden>False</Hidden>
    </btDstFileProcess>
  </actionToolList>
</SPIDecodeTool>